{
  "name": "react-server-side-starter",
  "version": "1.0.0",
  "description": "Starter kit for server side rendering of react redux application, It will return the first page rendered from server so as to support SEO for the website.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --progress && node src/server/index.js",
    "sample": "better-npm-run start-dev"
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack --progress && node src/server/index.js",
      "env": {
        "PORT": 8084
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksibesh/react-server-side-starter.git"
  },
  "author": "ksibesh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ksibesh/react-server-side-starter/issues"
  },
  "homepage": "https://github.com/ksibesh/react-server-side-starter#readme",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.10",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "http-proxy": "^1.14.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2"
  }
}
